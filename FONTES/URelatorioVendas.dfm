  object Query1: TQuery
    DatabaseName = 'SysEgg'
    SQL.Strings = (
      'SELECT B.IT_REF, B.IT_DESCRICAO,'
      'SUM(IT_QTDOVO) AS QTDOVO'
      'FROM PEDIDOS A, ITENS B'
      
        'WHERE A.PED_NUMERO = B.IT_NUMERO AND A.PED_DATA BETWEEN :VARDATA' +
        'I AND :VARDATAF'
      'GROUP BY B.IT_REF, B.IT_DESCRICAO'
      'ORDER BY B.IT_REF')
    Left = 480
    Top = 24
    ParamData = <
      item
        DataType = ftDate
        Name = 'VARDATAI'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'VARDATAF'
        ParamType = ptUnknown
      end>
    object Query1IT_REF: TStringField
      FieldName = 'IT_REF'
      Origin = 'SYSEGG."Itens.DB".It_Ref'
      Size = 8
    end
    object Query1IT_DESCRICAO: TStringField
      FieldName = 'IT_DESCRICAO'
      Origin = 'SYSEGG."Itens.DB".It_Descricao'
      Size = 50
    end
    object Query1QTDOVO: TFloatField
      FieldName = 'QTDOVO'
      Origin = 'SYSEGG."Itens.DB".It_QtdOvo'
