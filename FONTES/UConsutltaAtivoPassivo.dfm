  object QueryEstoque: TQuery
    DatabaseName = 'SysEgg'
    SQL.Strings = (
      
        'SELECT PRO_REF AS REF, PRO_DESCRICAO AS DESCRICAO, PRO_QTDATCAIX' +
        'A AS QTDCX,'
      '(PRO_QTDATCAIXA * PRO_PRECOMEDIO * 30) AS VALORCUSTO'
      'FROM PRODUTOS'
      'ORDER BY PRO_REF')
    Left = 124
    Top = 155
    object QueryEstoqueREF: TStringField
      FieldName = 'REF'
      Origin = 'SYSEGG."Produtos.DB".Pro_Ref'
      Size = 8
    end
    object QueryEstoqueDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Origin = 'SYSEGG."Produtos.DB".Pro_Descricao'
      Size = 50
    end
    object QueryEstoqueQTDCX: TFloatField
      FieldName = 'QTDCX'
      Origin = 'SYSEGG."Produtos.DB".Pro_QtdAtCaixa'
    end
    object QueryEstoqueVALORCUSTO: TFloatField
      FieldName = 'VALORCUSTO'
      Origin = 'SYSEGG."Produtos.DB".Pro_QtdAtCaixa'
      currency = True
    end
  end
  object QueryPendencias: TQuery
    OnCalcFields = QueryPendenciasCalcFields
    DatabaseName = 'SysEgg'
    SQL.Strings = (
      'SELECT PED_DATA, PED_CLIENTE, VALOR1, VALOR2, VALOR3,'
      'PAGO1, PAGO2, PAGO3'
      'FROM PEDIDOS'
      'WHERE PED_CLIENTE <> '#39'VENDA BALC'#195'O'#39' AND Ped_ForPaga <> '#39#193' VISTA'#39
      'ORDER BY PED_DATA')
