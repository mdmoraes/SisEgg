  object QryFaturamento: TQuery
    CachedUpdates = True
    DatabaseName = 'SysEgg'
    SQL.Strings = (
      
        'SELECT Ped_Data,  Ped_VrFinal as TOT_PEDIDO, PED_LUCRO as TOT_LU' +
        'CRO'
      'FROM "Pedidos.DB" Pedidos'
      'WHERE Ped_Data BETWEEN :VARDATAI AND :VARDATAF'
      'order by Ped_Data')
    UpdateMode = upWhereChanged
    Left = 416
    Top = 6
    ParamData = <
      item
        DataType = ftDate
        Name = 'VARDATAI'
        ParamType = ptUnknown
      end
      item
        DataType = ftDate
        Name = 'VARDATAF'
        ParamType = ptUnknown
      end>
    object QryFaturamentoPed_Data: TDateField
      FieldName = 'Ped_Data'
      Origin = 'DEBORA."Pedidos.DB".Ped_Data'
    end
    object QryFaturamentoTOT_LUCRO: TCurrencyField
      FieldName = 'TOT_LUCRO'
      Origin = 'DEBORA."Pedidos.DB".Ped_Lucro'
    end
    object QryFaturamentoTOT_PEDIDO: TCurrencyField
